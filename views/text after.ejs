
<%- include ('header.ejs') %> 
<div class="w-full max-w-lg m-auto mt-40">
<form method="POST" action="/addDataAfter" class="bg-white shadow-lg rounded px-8 pt-6 pb-8 mb-4">

    <div class="mb-4">
      <label class="block text-gray-700 text-sm font-bold mb-2 text-2xl" for="dataAfter">
        Add Data After Each Number
      </label>
      <input type="text" name="dataAfter" placeholder="enter data to put before each number" class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" autocomplete="off" required>
    </div>
    <button type="submit" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded focus:outline-none focus:shadow-outline w-full">Add  </button>
   </form>
</div>
   <%- include ('footer.ejs') %> 
